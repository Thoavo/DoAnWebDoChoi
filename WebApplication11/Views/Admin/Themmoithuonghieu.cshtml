@model WebApplication11.Models.THUONGHIEU

@{
    ViewBag.Title = "Themmoithuonghieu";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<script src="~/vendors/ckeditor/ckeditor.js"></script>
<script src="~/vendors/ckeditor/adapters/jquery.js"></script>


<div class="col-md-10 panel-info">
    <div class="content-box-large box-with-header">
        @using (Html.BeginForm("Themmoithuonghieu", "Admin", FormMethod.Post, new { @enctype = "multipart/form-data" }))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <h4>Thêm mới thương hiệu</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(model => model.Tenthuonghieu, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.EditorFor(model => model.Tenthuonghieu, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.Tenthuonghieu, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10 ">
                        <input type="submit" value="Thêm mới" class="btn btn-primary" />
                    </div>
                </div>
            </div>
        }

        <div>
            @Html.ActionLink("Về trang quản lý thương hiệu", "QLThuonghieu")
        </div>
    </div>
</div>